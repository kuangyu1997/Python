#!/usr/bin/python
# -*- coding: UTF-8 -*-

import time

print time.strftime('%Y-%m-%d %H:%M:%S',time.localtime(time.time()))

#暂停一秒
time.sleep(1)

print time.strftime('%Y-%m-%d %H:%M:%S',time.localtime(time.time()))

time.sleep(2)

print time.strftime('%Y-%m-%d %H:%M:%S',time.localtime(time.time()))

time.sleep(3)

print time.strftime('%Y-%m-%d %H:%M:%S',time.localtime(time.time()))


